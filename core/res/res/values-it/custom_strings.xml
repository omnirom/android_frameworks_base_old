<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- MicroG -->
    <string name="permlab_fakePackageSignature">Spoof firma del pacchetto</string>
    <!-- Description of an application permission, listed so the user can choose whether
         they want to allow the application to do this. -->
    <string name="permdesc_fakePackageSignature">Consente all\'applicazione di fingere di essere un\'altra app. Applicazioni dannose potrebbero essere in grado di utilizzare questo per accedere ai dati privati delle applicazioni. Gli usi legittimi includono un emulatore che finge di essere quello che emula. Concedi questo permesso con cautela!</string>
    <!-- Title of a category of application permissions, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permgrouplab_fake_package_signature">Spoof firma del pacchetto</string>
    <!-- Description of a category of application permissions, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permgroupdesc_fake_package_signature">permette di effettuare uno spoof della firma del pacchetto</string>
    <!-- Message shown to the user when the apps requests permission from this group. If ever possible this should stay below 80 characters (assuming the parameters takes 20 characters). Don't abbreviate until the message reaches 120 characters though. [CHAR LIMIT=120] -->
    <string name="permgrouprequest_fake_package_signature">Consentire
        &lt;b&gt;<xliff:g id="app_name" example="Gmail">%1$s</xliff:g>&lt;/b&gt; di effettuare uno spoof della firma del pacchetto?</string>
    <!-- advanced reboot to bootloader. -->
    <string name="reboot_to_bootloader_title">Riavvia in bootloader</string>
    <string name="reboot_to_bootloader_message">Riavvio in bootloader\u2026</string>
    <!-- advanced reboot to recovery. -->
    <string name="reboot_to_recovery_title">Riavvia in recovery</string>
    <string name="reboot_to_recovery_message">Riavvio in recovery\u2026</string>
    <!-- advanced reboot system -->
    <string name="reboot_system_title">Riavvia il sistema</string>
    <string name="reboot_system_message">Riavvio in sistema\u2026</string>
    <!-- advanced reboot to fastboot. -->
    <string name="reboot_to_fastboot_title">Riavvia in fastboot</string>
    <string name="reboot_to_fastboot_message">Riavvio in fastboot\u2026</string>
</resources>
