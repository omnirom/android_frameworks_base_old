<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- minimal way before long swipe will get accepted -->
    <dimen name="nav_gesture_swipe_min_length">60dp</dimen>
    <!-- maximum way that long press get accepted -->
    <dimen name="nav_gesture_long_press_max_length">55dp</dimen>
    <!-- time in ms that needs to pass before trigger the action -->
    <integer name="nav_gesture_swipe_timout">250</integer>
    <!-- signification move threshold - if stays below will trigger action -->
    <dimen name="nav_gesture_move_threshold">2dp</dimen>
</resources>
