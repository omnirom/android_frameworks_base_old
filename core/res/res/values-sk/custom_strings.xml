<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- MicroG -->
    <string name="permlab_fakePackageSignature">Napodobniť podpis balíčka</string>
    <!-- Description of an application permission, listed so the user can choose whether
         they want to allow the application to do this. -->
    <string name="permdesc_fakePackageSignature">Zámena podpisu balíčku umožňuje aplikácii predstierať, že je iná aplikácia. Tieto aplikácie môžu byť schopné pristupovať k súkromným dátam aplikácií. S udelením tohto oprávnenia opatrne!</string>
    <!-- Title of a category of application permissions, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permgrouplab_fake_package_signature">Napodobniť podpis balíčka</string>
    <!-- Description of a category of application permissions, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permgroupdesc_fake_package_signature">Podpis balíčka je možné zmeniť</string>
    <!-- Message shown to the user when the apps requests permission from this group. If ever possible this should stay below 80 characters (assuming the parameters takes 20 characters). Don't abbreviate until the message reaches 120 characters though. [CHAR LIMIT=120] -->
    <string name="permgrouprequest_fake_package_signature">Povoliť aplikácii
        &lt;b&gt;<xliff:g id="app_name" example="Gmail">%1$s</xliff:g>&lt;/b&gt; zmeniť podpis balíčku?</string>
    <!-- advanced reboot to bootloader. -->
    <string name="reboot_to_bootloader_title">Reštart do Bootloadera</string>
    <string name="reboot_to_bootloader_message">Reštartovanie do bootloadera\u2026</string>
    <!-- advanced reboot to recovery. -->
    <string name="reboot_to_recovery_title">Reštart do Recovery</string>
    <string name="reboot_to_recovery_message">Reštartovanie do recovery\u2026</string>
    <!-- advanced reboot system -->
    <string name="reboot_system_title">Reštart Systému</string>
    <string name="reboot_system_message">Reštartovanie systému\u2026</string>
    <!-- advanced reboot to fastboot. -->
    <string name="reboot_to_fastboot_title">Reštart do režimu fastboot</string>
    <string name="reboot_to_fastboot_message">Reštartovanie do režimu fastboot\u2026</string>
</resources>
