<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Do not translate. Defines the mapping of notification package names
         from the actual triggering package to the user selectable package.
         E.g. GTalk notifications come via Google Services Framework
         Format: [triggering package]|[user package] -->
    <string-array name="notification_light_package_mapping" translatable="false">
        <item>com.google.android.gsf|com.google.android.talk</item>
    </string-array>

    <!-- Defines all possible global actions -->
    <string-array translatable="false" name="values_globalActionsList">
        <item>power</item>
        <item>reboot</item>
        <item>lockdown</item>
        <item>settings</item>
        <item>airplane</item>
        <item>screenshot</item>
        <item>screenrecord</item>
        <item>torch</item>
        <item>users</item>
        <item>voiceassist</item>
        <item>silent</item>
        <item>dnd</item>
    </string-array>

    <string-array name="entries_battery_styles" translatable="false">
        <item>@string/battery_style_default</item>
        <item>@string/battery_style_meter_horizontal</item>
        <item>@string/battery_style_circle</item>
        <item>@string/battery_style_text</item>
        <item>@string/battery_style_droid</item>
        <item>@string/battery_style_theme</item>
    </string-array>

    <string-array name="values_battery_styles" translatable="false">
        <item>0</item>
        <item>1</item>
        <item>2</item>
        <item>3</item>
        <item>4</item>
        <item>5</item>
    </string-array>
</resources>
