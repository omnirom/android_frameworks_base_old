<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Action bar title when the app is in standalone (file manager) mode [CHAR LIMIT=32] -->
    <string name="title_standalone">Файлы</string>

    <!-- Toast shown after successfully deleting files -->
    <string name="toast_success_delete">Файлы удалены</string>

    <!-- Title of the Delete confirmation dialog -->
    <string name="dialog_delete_confirm_title">Удалить файлы?</string>

    <!-- Text shown in a toast when the user copies files -->
    <plurals name="files_copied">
        <item quantity="one">Файл скопирован в буфер обмена</item>
        <item quantity="other">%d файла(ов) скопированы в буфер обмена</item>
    </plurals>

    <!-- Text shown in a toast when the user pastes files -->
    <plurals name="files_pasted">
        <item quantity="one">Файл вставлен. Загрузка может быть завершена в фоне.</item>
        <item quantity="other">%d файла(ов) вставлено. Загрузка может быть завершена в фоне.</item>
    </plurals>

    <!-- Text show in the progress dialog while files are being copied -->
    <string name="copy_in_progress">Копирование\u2026</string>

    <!-- Text show in the progress dialog while files are being deleted -->
    <string name="delete_in_progress">Удаление\u2026</string>

    <plurals name="dialog_delete_confirm_message">
        <item quantity="one">Это действие необратимо. Подтвердите удаление файла</item>
        <item quantity="other">Это действие необратимо. Подтвердите удаление %d файлов</item>
    </plurals>
</resources>
